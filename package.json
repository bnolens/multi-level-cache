{
  "name": "multi-level-cache",
  "version": "0.0.1",
  "description": "Manage local and remote caches with a single API",
  "main": "index.js",
  "scripts": {
    "pre-commit-update": "node_modules/.bin/npm-update-outdated",
    "test": "mocha --recursive",
    "posttest": "jshint .",
    "jshint": "jshint .",
    "check-coverage": "istanbul check-coverage --statements 84 --branches 80 --functions 65 --lines 84 ./coverage/coverage.json",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha -- --recursive",
    "pre-commit-stash-save": "git stash --keep-index",
    "post-commit-stash-pop": "git stash pop"
  },
  "pre-commit": [
    "pre-commit-update",
    "jshint",
    "pre-commit-stash-save",
    "coverage",
    "post-commit-stash-pop",
    "check-coverage"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/guyellis/multi-level-cache.git"
  },
  "keywords": [
    "cache",
    "node",
    "redis",
    "memcached"
  ],
  "author": "Guy Ellis <wildfiction@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guyellis/multi-level-cache/issues"
  },
  "homepage": "https://github.com/guyellis/multi-level-cache",
  "dependencies": {
    "async": "^0.9.0",
    "debug": "^2.1.3",
    "lodash": "^3.6.0",
    "node-cache": "^1.1.0",
    "redis": "^0.12.1"
  },
  "devDependencies": {
    "istanbul": "^0.3.13",
    "jshint": "^2.6.3",
    "mocha": "^2.2.4",
    "npm-update-outdated": "^0.1.4",
    "pre-commit": "^1.0.6",
    "sinon" : "^1.14.1"
  },
  "engines": {
    "node": ">= 0.10.0"
  }
}
